{
  "compilerOptions": {
    "strict": true,
    "module": "es6",
    "baseUrl": ".", // this must be specified if "paths" is specified.
    "paths": {
      "@env": ["node_modules/react-native-dotenv"], // this mapping is relative to "baseUrl"
      "@components/*": ["components/*"],
      "@screens/*": ["screens/*"],
      "assets/*": ["assets/*"],
    }
  },
  "extends": "expo/tsconfig.base"
}
